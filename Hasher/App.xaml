<Application x:Class="Hasher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Hasher"
             StartupUri="pack://application:,,,/Views/Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Icons.Hasher.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Styles.App.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Styles.ToolBar.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Styles.StatusBar.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>